{
  "name": "serverless-aws-nodejs-typescript",
  "version": "1.0.0",
  "main": "handler.js",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/mesaugat/serverless-aws-nodejs-typescript",
  "scripts": {
    "test:unit": "jest unit",
    "test:func": "jest func",
    "tsc": "tsc --noEmit",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "deploy": "cross-env NODE_OPTIONS=--max_old_space_size=3096 sls deploy --force"
  },
  "dependencies": {
    "@acuris/aws-es-connection": "^1.0.1",
    "@aws/dynamodb-auto-marshaller": "^0.7.1",
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "@elastic/elasticsearch": "^7.4.0",
    "@octokit/rest": "^16.35.0",
    "base-converter": "^1.1.2",
    "lodash-es": "^4.17.15",
    "lucene-escape-query": "^1.0.1",
    "luxon": "^1.11.3",
    "node-fetch": "^2.6.0",
    "yaml": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@types/aws-lambda": "8.10.15",
    "@types/jest": "^24.0.23",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lodash-es": "^4.17.3",
    "@types/luxon": "^1.11.1",
    "@types/node": "^11.9.5",
    "@types/node-fetch": "^2.5.4",
    "@types/yaml": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "aws-sdk": "^2.578.0",
    "babel-jest": "23.6.0",
    "cache-loader": "^3.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.9.0",
    "jest-junit": "^6.3.0",
    "serverless": "^1.58.0",
    "serverless-offline": "^3.31.3",
    "serverless-plugin-scripts": "^1.0.2",
    "serverless-plugin-tracing": "^2.0.0",
    "serverless-pseudo-parameters": "^2.5.0",
    "serverless-webpack": "^5.3.1",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "webpack": "^4.41.0",
    "webpack-node-externals": "1.7.2"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/*(*.)@(spec|test).[tj]s?(x)"
    ],
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "transform": {
      "^.+\\.(tsx?|json)$": "ts-jest",
      "^.+\\.(jsx?)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es/.*)"
    ]
  }
}
